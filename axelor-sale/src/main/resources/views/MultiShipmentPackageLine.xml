<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="multi-shipment-package-line-grid" title="Multi Shipment Package Line"
    model="com.axelor.apps.sale.db.MultiShipmentPackageLine">
    <button onClick="action-multi-shipment-line-print-lable" name="printBtn" title="Print"
      icon="fa-envelope"/>
    <field name="trackingNumber"/>
    <field name="lableUrl"/>
  </grid>

  <form name="multi-shipment-package-line-form" title="Multi Shipment Package Line"
    model="com.axelor.apps.sale.db.MultiShipmentPackageLine">
    <panel name="packPanel">
      <field name="trackingNumber"/>
      <field name="lableUrl" colSpan="12" readonly="false">
        <viewer depends="lableUrl"><![CDATA[
		      <div>
		        <span ng-show="record.lableUrl != null" ><a href={{record.lableUrl}} target="_blank">Lable Link</a></span>
		      </div>
		    ]]></viewer>
        <editor colSpan="12" showTitle="false">
          <field name="lableUrl"/>
        </editor>
      </field>
      <field name="lableUrl" colSpan="12"/>
    </panel>
  </form>

  <action-method name="action-multi-shipment-line-print-lable">
    <call class="com.axelor.apps.sale.web.MultiShipmentPackageLineController"
      method="prtinLable"/>
  </action-method>

</object-views>
